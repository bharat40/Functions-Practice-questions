#include <stdio.h> // function to find sum of two numbers
int sum(int, int);
int main()
{
    printf("Function basic structure");
    printf("\n-------------------------\n");
    int total, n1, n2;
    printf("Enter two nmbers : ");
    scanf("%d %d", &n1, &n2);
    // function call
    total = sum(n1, n2);
    printf("Sum = %d", total);
    return 0;
}

int sum(int a, int b)
{
    int s;
    s = a + b;
    return s;
}
